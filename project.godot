; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Diffusal"
config/description="Built for LD Jam 51. You are a bomb diffusal effort with a knack for discovering bombs just as their about to explode. Can you diffuse the 9 different bombs?"
boot_splash/image="res://Marketing/gd-splash.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.952941, 0.835294, 0.0117647, 1 )
config/icon="res://Sprites/bomb.png"

[debug]

gdscript/warnings/treat_warnings_as_errors=true

[display]

window/size/width=256
window/size/height=224
window/size/test_width=1028
window/size/test_height=896
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/use_hidpi=true

[input]

Click={
"deadzone": 0.5,
"events": [ Object(InputEventMouseButton,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"button_mask":0,"position":Vector2( 0, 0 ),"global_position":Vector2( 0, 0 ),"factor":1.0,"button_index":1,"pressed":false,"doubleclick":false,"script":null)
 ]
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/fallback_to_gles2=true
2d/options/use_nvidia_rect_flicker_workaround=true
quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=0
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
misc/lossless_compression/force_png=true
environment/default_environment="res://default_env.tres"
